              <span style="font-size: 10px;">({{#if Pcs.clusterController.show_all_resources}}<%=@I18n.t(:'promptInfo.displayAll')%>{{else}}<%=@I18n.t(:'promptInfo.displayIssue')%>{{/if}})</span>
